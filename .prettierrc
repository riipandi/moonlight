{
    "$schema": "http://json.schemastore.org/prettierrc",
    "jsxSingleQuote": true,
    "arrowParens": "always",
    "bracketSpacing": true,
    "printWidth": 100,
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false,
    "plugins": ["prettier-config-moon", "prettier-plugin-tailwindcss"]
}
