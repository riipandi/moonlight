{
    "name": "moonlight",
    "version": "0.2.0",
    "private": true,
    "scripts": {
        "deps": "npx --yes npm-check-updates -x typescript --deep --interactive",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
        "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint . --fix",
        "format": "prettier --write \"**/*.{json,js,jsx,ts,tsx}\"",
        "moon": "moon --color --log trace"
    },
    "devDependencies": {
        "@moonrepo/cli": "^1.13.5",
        "@types/jest": "^29.5.5",
        "@types/node": "^20.6.5",
        "babel-preset-moon": "^2.0.8",
        "eslint-config-moon": "^2.0.9",
        "eslint": "^8.50.0",
        "jest-preset-moon": "^2.0.4",
        "jest": "^29.7.0",
        "prettier-config-moon": "^1.1.2",
        "prettier-plugin-tailwindcss": "^0.5.4",
        "prettier": "^3.0.3",
        "tsconfig-moon": "^1.3.0",
        "typescript": "5.0.4"
    },
    "packageManager": "pnpm@8.6.5",
    "engines": {
        "node": "18.16.1"
    }
}
