{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Next.js",
    "extends": "./base.json",
    "compilerOptions": {
        "allowJs": true,
        "emitDeclarationOnly": false,
        "incremental": true,
        "jsx": "preserve",
        "lib": ["dom", "dom.iterable", "esnext"],
        "module": "esnext",
        "noEmit": false,
        "plugins": [{ "name": "next" }],
        "resolveJsonModule": true,
        "target": "es5"
    }
}
